     <xs:minLength value="10"/>
     <xs:maxLength value="24"/>
	   </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="stop_local_true_solar_time">
    <xs:annotation>
      <xs:documentation>Stop_local_true_solar_time is the local true
