     <xs:minLength value="8"/>
     <xs:maxLength value="24"/>
	   </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="stop_sol_number">
    <xs:annotation>
      <xs:documentation>Stop_sol_number is the number of the Mars day on
